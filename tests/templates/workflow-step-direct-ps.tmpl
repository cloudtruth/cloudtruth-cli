
      - name: {{ os }}-{{ version }}
        shell: pwsh
        run: |
          $ENV:CT_DOWNLOAD_RELEASE_ID = Get-Content .\artifacts\release-id -Raw
          $ENV:CT_DOWNLOAD_VERSION = Get-Content .\artifacts\release-version -Raw
          .\install.ps1 -authToken "$ENV:CT_DOWNLOAD_AUTH_TOKEN" -releaseId "$ENV:CT_DOWNLOAD_RELEASE_ID" -version "$ENV:CT_DOWNLOAD_VERSION"
          New-Alias grep findstr
          & "$ENV:TEMP\cloudtruth.exe" --project "$ENV:CT_TEST_PROJECT" parameters list -v | grep "$ENV:CT_TEST_PARAM" | grep "$ENV:CT_TEST_VALUE"

